{
  "name": "pbv-starter",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm watch:build\" \"pnpm dev:server\" ",
    "dev:client": "cd client && pnpm dev",
    "dev:server": "air",
    "build": "cd client && pnpm build",
    "preview": "cd client && pnpm preview",
    "watch:build": "cd client && pnpm build --watch"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
